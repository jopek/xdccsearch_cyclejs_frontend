{
    "name": "xdccsearch-cycle-frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@cycle/dom": ">=20.4.0",
        "@cycle/history": ">=6.10.0",
        "@cycle/http": ">=14.10.0",
        "@cycle/isolate": ">=3.4.0",
        "@cycle/run": ">=4.4.0",
        "@cycle/time": "^0.19.0",
        "bootstrap": "^4.3.1",
        "cycle-restart": "0.2.3",
        "cycle-scripts-one-fits-all": "^7.2.1",
        "cycle-storageify": ">=4.0.1",
        "cyclejs-test-helpers": "^4.0.0",
        "cyclejs-utils": "^4.0.2",
        "cyclic-router": "^6.0.0",
        "html-looks-like": "1.0.3",
        "husky": "^2.1.0",
        "jquery": "^3.4.0",
        "jsverify": ">=0.8.4",
        "lint-staged": "^8.1.5",
        "popper.js": "^1.15.0",
        "prettier": ">=1.17.0",
        "snabbdom-to-html": ">=5.1.1",
        "sockjs": "^0.3.19",
        "sockjs-client": "^1.3.0",
        "switch-path": ">=1.2.0",
        "vertx3-eventbus-client": "^3.7.0",
        "xstream": ">=11.10.0"
    },
    "devDependencies": {},
    "scripts": {
        "precommit": "lint-staged",
        "format": "prettier --write '{src,test}/**/*.{js,jsx,ts,tsx}'",
        "start": "cycle-scripts start",
        "test": "cycle-scripts test",
        "build": "cycle-scripts build",
        "eject": "cycle-scripts eject",
        "clean": "cycle-scripts clean"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "git add"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true
    },
    "nyc": {
        "instrument": false,
        "sourceMap": false,
        "include": [
            "src/components"
        ],
        "reporter": [
            "html",
            "text-summary"
        ]
    },
    "mocha-webpack": {
        "include": [
            "src/components/**/*.{jsx,js,ts,tsx}",
            "test/**/*.test.{js,jsx,ts,tsx}"
        ]
    }
}
